{
  "name": "dtd",
  "version": "1.0.0",
  "description": "Docs-first, AI-assisted directory connecting Melbourne dog owners to trainers, behaviour consultants, and emergency resources across 28 councils. Built on Next.js 14 App Router, Supabase (Postgres/Auth/Functions/Storage), and Stripe-ready (monetization deferred) with strong disclaimers (no SLAs).",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "validate-import": "python3 scripts/validate_import.py",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:start": "bash ./scripts/local_db_start_apply.sh --reset",
    "db:seed": "bash ./scripts/local_db_start_apply.sh --seed-only",
    "dev:local": "npm run db:start & sleep 1 && next dev",
    "allowlist:staging": "python3 scripts/generate_allowlist.py staging",
    "allowlist:prod": "python3 scripts/generate_allowlist.py prod",
    "abn:batch:staging": "python3 scripts/abn_controlled_batch.py --file=scripts/controlled_abn_list.staging.json",
    "abn:batch:staging:apply": "AUTO_APPLY=true python3 scripts/abn_controlled_batch.py --apply --file=scripts/controlled_abn_list.staging.json",
    "abn:batch:prod": "python3 scripts/abn_controlled_batch.py --file=scripts/controlled_abn_list.prod.json",
    "abn:batch:prod:apply": "AUTO_APPLY=true python3 scripts/abn_controlled_batch.py --apply --file=scripts/controlled_abn_list.prod.json",
    "test:integration": "vitest run src/tests/integration --threads=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlsuburbmates/dogtrainersdirectory.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "type": "module",
  "bugs": {
    "url": "https://github.com/carlsuburbmates/dogtrainersdirectory/issues"
  },
  "homepage": "https://github.com/carlsuburbmates/dogtrainersdirectory#readme",
  "dependencies": {
    "@supabase/auth-helpers-nextjs": "^0.15.0",
    "@supabase/auth-helpers-react": "^0.15.0",
    "@supabase/supabase-js": "^2.86.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.5",
    "next": "^16.0.5",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "stripe": "^11.18.0",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "pg": "^8.16.3",
    "vitest": "^4.0.14"
  }
}
